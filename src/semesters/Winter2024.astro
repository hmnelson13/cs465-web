---
import Layout from "../layouts/Front.astro";
import NavBar from "../menu/NavBar.astro";

const semester = "Winter 2024";
const prefix = '/winter2024';
const items = [
  {
    title: "Home",
    url: "/",
  },
  {
    title: "Syllabus",
    url: "/syllabus",
  },
  {
    title: "Homework",
    url: "/homework",
    submenu: [
      {
        title: "test",
        url: "/homework/test",
      },
    ],
  },
  {
    title: "Projects",
    url: "/projects",
    submenu: [
      {
        title: "test",
        url: "/projects/test",
      },
    ],
  },
];

---



<Layout>
  <NavBar semester={semester} prefix={prefix} items={items} />
  <div class="sm:bg-blue-300 min-h-screen pt-0 lg:pt-5 pb-5">
    <div
      class="bg-white ml-5 mr-5 sm:px-[75px] sm:ml-auto sm:mr-auto py-3 max-w-5xl rounded-lg"
    >
        <slot />
    </div>
  </div>
</Layout>
