---
import Burger from "./Burger.jsx";
import Header from "./Header.jsx";
const { semester, prefix, items } = Astro.props;
---

<nav
  class="h-55 bg-blue-100 border-gray-400 border-b-2 px-5 py-3 md:px-10 flex justify-between"
>
  <Header semester={semester} />
  <Burger prefix={prefix} items={items} client:load />
</nav>
